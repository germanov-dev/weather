='/moon-phase-3.svg' width={21} height={21} />;\n\t\t\tcase phase > 0.37 && phase <= 0.5:\n\t\t\t\treturn <Image src='/moon-phase-4.svg' width={21} height={21} />;\n\t\t\tcase phase > 0.5 && phase <= 0.62:\n\t\t\t\treturn <Image src='/moon-phase-5.svg' width={21} height={21} />;\n\t\t\tcase phase > 0.62 && phase <= 0.75:\n\t\t\t\treturn <Image src='/moon-phase-6.svg' width={21} height={21} />;\n\t\t\tcase phase > 0.75 && phase <= 0.87:\n\t\t\t\treturn <Image src='/moon-phase-7.svg' width={21} height={21} />;\n\t\t\tcase phase > 0.87 && phase <= 1:\n\t\t\t\treturn <Image src='/moon-phase-8.svg' width={21} height={21} />;\n\t\t\tdefault:\n\t\t\t\tbreak;\n\t\t}\n\t};\n\n\tconst chartDotPosition = (temp, tempHigh, tempLow) => {\n\t\tconsole.log(temp, tempHigh, tempLow);\n\n\t\tswitch (true) {\n\t\t\tcase temp == tempHigh:\n\t\t\t\treturn 80;\n\t\t\tcase temp >= tempHigh * 0 && temp <= tempHigh * 0.05:\n\t\t\t\treturn 4.5;\n\t\t\tcase temp >= tempHigh * 0.05 && temp <= tempHigh * 0.1:\n\t\t\t\treturn 9;\n\t\t\tcase temp >= tempHigh * 0.1 && temp <= tempHigh * 0.15:\n\t\t\t\treturn 13.5;\n\t\t\tcase temp >= tempHigh * 0.15 && temp <= tempHigh * 0.2:\n\t\t\t\treturn 18;\n\t\t\tcase temp >= tempHigh * 0.2 && temp <= tempHigh * 0.25:\n\t\t\t\treturn 22.5;\n\t\t\tcase temp >= tempHigh * 0.25 && temp <= tempHigh * 0.3:\n\t\t\t\treturn 27;\n\t\t\tcase temp >= tempHigh * 0.3 && temp <= tempHigh * 0.35:\n\t\t\t\treturn 31.5;\n\t\t\tcase temp >= tempHigh * 0.35 && temp <= tempHigh * 0.4:\n\t\t\t\treturn 36;\n\t\t\tcase temp >= tempHigh * 0.4 && temp <= tempHigh * 0.45:\n\t\t\t\treturn 40.5;\n\t\t\tcase temp >= tempHigh * 0.45 && temp <= tempHigh * 0.5:\n\t\t\t\treturn 45;\n\t\t\tcase temp >= tempHigh * 0.5 && temp <= tempHigh * 0.55:\n\t\t\t\treturn 49.5;\n\t\t\tcase temp >= tempHigh * 0.55 && temp <= tempHigh * 0.6:\n\t\t\t\treturn 54;\n\t\t\tcase temp >= tempHigh * 0.6 && temp <= tempHigh * 0.65:\n\t\t\t\treturn 58.5;\n\t\t\tcase temp >= tempHigh * 0.65 && temp <= tempHigh * 0.7:\n\t\t\t\treturn 63;\n\t\t\tcase temp >= tempHigh * 0.7 && temp <= tempHigh * 0.75:\n\t\t\t\treturn 67.5;\n\t\t\tcase temp >= tempHigh * 0.75 && temp <= tempHigh * 0.8:\n\t\t\t\treturn 72;\n\t\t\tcase temp >= tempHigh * 0.8 && temp <= tempHigh * 0.85:\n\t\t\t\treturn 76.5;\n\t\t\tcase temp >= tempHigh * 0.85 && temp <= tempHigh * 0.9:\n\t\t\t\treturn 81;\n\t\t\tcase temp >= tempHigh * 0.9 && temp <= tempHigh * 0.95:\n\t\t\t\treturn 85.5;\n\t\t\tcase temp == tempLow:\n\t\t\t\treturn 0;\n\t\t\tdefault:\n\t\t\t\treturn;\n\t\t}\n\t};\n\n\tconst detailedWeather = !loading ? (\n\t\t<>\n\t\t\t<div className={styles.DetailedWeather}>\n\t\t\t\t<div>\n\t\t\t\t\t<div className={styles.Date}>\n\t\t\t\t\t\t{formatDate(weather.daily.data[0].time)}\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div className={styles.Location}>\n\t\t\t\t\t\t{cityName}, {countryName}\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div>{weatherIcon(weather.daily.data[0].icon)}</div>\n\n\t\t\t\t\t<div className={styles.Summary}>{weather.daily.data[0].summary}</div>\n\n\t\t\t\t\t<div className={styles.Row}>\n\t\t\t\t\t\t<div className={styles.Temp}>\n\t\t\t\t\t\t\t<Image src='/temp-high.svg' width={70} height={70} />\n\t\t\t\t\t\t\tHigh:{' '}\n\t\t\t\t\t\t\t<strong>\n\t\t\t\t\t\t\t\t{Math.round(weather.daily.data[0].temperatureHigh)}\n\t\t\t\t\t\t\t</strong>{' '}\n\t\t\t\t\t\t\tat{' '}\n\t\t\t\t\t\t\t<strong>\n\t\t\t\t\t\t\t\t{formatTime(weather.daily.data[0].temperatureHighTime)}\n\t\t\t\t\t\t\t</strong>\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t<div className={styles.Temp}>\n\t\t\t\t\t\t\t<Image src='/temp-low.svg' width={70} height={70} />\n\t\t\t\t\t\t\tLow:{' '}\n\t\t\t\t\t\t\t<strong>\n\t\t\t\t\t\t\t\t{Math.round(weather.daily.data[0].temperatureLow)}\n\t\t\t\t\t\t\t</strong>{' '}\n\t\t\t\t\t\t\tat{' '}\n\t\t\t\t\t\t\t<strong>\n\t\t\t\t\t\t\t\t{formatTime(weather.daily.data[0].temperatureLowTime)}\n\t\t\t\t\t\t\t</strong>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t\t<ul className={styles.DetailedList}>\n\t\t\t\t\